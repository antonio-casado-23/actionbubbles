<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="omtinez.routerremote"
      android:versionName="1.6"
      android:versionCode="10">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
<uses-sdk android:minSdkVersion="4" android:targetSdkVersion="17" />

    <application android:icon="@drawable/icon"
        		android:label="@string/app_name"
        		android:theme="@style/Theme.Sherlock"
        		android:allowBackup="true">
        		
        <activity android:name="LoginActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="omtinez.routerremote.ListActivity" />
        <activity android:name="omtinez.routerremote.MenuActivity" android:screenOrientation="portrait" />
        <activity android:name="omtinez.routerremote.CustomActivity" />

    </application>
</manifest>